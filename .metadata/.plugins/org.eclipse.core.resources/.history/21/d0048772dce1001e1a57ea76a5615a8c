#include	"r01lib.h"
r01lib_start;	/* *** place this word before making instance of r01lib classes *** */

#include	<core_cm33.h>

DigitalOut	r( RED   );

int main(void)
{
	while ( true )
	{
		r	= !r;
		wait( 0.2 );
	}
}
