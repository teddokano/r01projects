#include	"r01lib.h"
r01lib_start;	/* *** place this word before making instance of r01lib classes *** */

Ticker		t;
DigitalOut	led( GREEN );

bool	flag_sw;
bool	flag_pin;

void callback_sw( void )
{
	flag_sw	= true;
}

void callback_pin( void )
{
	led	= !led;
}


int main(void)
{
	t.attach( callback, 0.1 );

	while ( true )
		;
}
